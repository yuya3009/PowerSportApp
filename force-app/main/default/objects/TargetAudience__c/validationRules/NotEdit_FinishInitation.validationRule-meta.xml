<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>NotEdit_FinishInitation</fullName>
    <active>true</active>
    <errorConditionFormula>AND(initiatiedFlag__c  = True
        ,NOT(ISCHANGED(initiatiedFlag__c)))</errorConditionFormula>
    <errorMessage>入信済の場合は対象者レコードを編集できません。
後輩レコードから編集してください</errorMessage>
</ValidationRule>
